      "version": "1.0",
      "capabilities": {
        "extract_product_name": "Linklerden ürün isimlerini çıkarma",
        "predict_hs_code": "Ürünün tahmini HS kodunu belirleme"
      }
    },
    "examples": [
      {
        "input_link": "https://www.example.com/products/laptop-gaming-rtx4060",
        "extracted_product": "Gaming Laptop RTX 4060",
        "predicted_hs_code": "8471.30"
      },
      {
        "input_link": "https://shop.example.com/electronics/smartphone-iphone15",
